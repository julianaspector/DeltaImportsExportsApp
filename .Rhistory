waterYear <- function(date, wyStart = 10) {
waterYear <- lubridate::year(date) + (lubridate::month(date) >
(wyStart - 1)) + offset - 1
return(waterYear)
}
waterYear(2012-09-30)
waterYear("2012-09-30")
